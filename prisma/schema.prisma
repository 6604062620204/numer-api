generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Root {
  id        Int      @id @default(autoincrement())
  solution  String
  xstart    Float?
  xend      Float?
  equation  String
  error     Float
  result    Float
  createdAt DateTime @default(now())
}

model Linear {
  id        Int      @id @default(autoincrement())
  solution  String
  matrixA   Json
  matrixB   Json
  equation  String
  createdAt DateTime @default(now())
}
